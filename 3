#include<iostream>
using namespace std;

int main ()
{
    //zad.3- По въведено число в диапазона 0 – 10, отпечатайте всички числа(от въведеното до 100) които са четни и кратни на 3
    int n;
    cin>>n;
    for(int i=n;i<100;i++)
    {
        if(i%2==0 && i%3==0)
        {
            cout<<i<<" ";
        }
    }
return 0;
}
